{
  "name": "sample-http",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "dev": "run-script-os",
    "dev:windows": "concurrently --raw \"npm run build:dev > nul\" \"npm run start:dev\"",
    "dev:nix": "concurrently --raw \"npm run build:dev > /dev/null\" \"npm run start:dev\"",
    "build:dev": "concurrently --raw \"tsc --watch\" \"npm run codegen:dev\"",
    "start:dev": "nodemon --watch dist/ --exec npm run preview",
    "start": "tsc || echo Typescript errors do not prevent the program from running & npm run preview",
    "build": "npm run codegen && tsc",
    "preview": "functions-framework --target=handler",
    "lint": "run-script-os",
    "lint:windows": "eslint . & prettier . -c",
    "lint:nix": "eslint .; prettier . -c",
    "lint-fix": "run-script-os",
    "lint-fix:windows": "eslint --fix . & prettier . --write",
    "lint-fix:nix": "eslint --fix .; prettier . --write",
    "codegen": "graphql-codegen --config codegen.ts --verbose",
    "codegen:dev": "graphql-codegen --config codegen.ts --watch"
  },
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@google-cloud/secret-manager": "^5.6.0",
    "@google-cloud/storage": "^7.8.0",
    "@graphql-tools/schema": "^10.0.6",
    "@graphql-tools/utils": "^10.5.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.1",
    "source-map-support": "^0.5.21",
    "typescript-eslint": "^8.4.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@google-cloud/functions-framework": "^3.3.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typescript": "4.0.9",
    "@graphql-codegen/typescript-resolvers": "4.4.0",
    "@parcel/watcher": "^2.4.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.5.4",
    "@types/source-map-support": "^0.5.10",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.0.0",
    "eslint": "^9.4.0",
    "prettier": "^3.3.2",
    "run-script-os": "^1.1.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
